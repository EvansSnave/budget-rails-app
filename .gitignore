# Ignore bundler config.
/.bundle
node_modules/

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

/node_modules
/yarn-error.log

.byebug_history
.env
/test-results/
/playwright-report/
/blob-report/
/playwright/.cache/
